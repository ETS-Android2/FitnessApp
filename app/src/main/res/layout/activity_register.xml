<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1a252b"
    tools:context=".register_activity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-190px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backgroundmain" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="308dp"
        android:layout_height="632dp"
        android:layout_marginTop="-180dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/walk_it_png" />

    <ImageView
        android:id="@+id/EmailContainer"
        android:layout_width="295dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usernameContainer"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangleshape" />

    <ImageView
        android:id="@+id/usernameContainer"
        android:layout_width="295dp"
        android:layout_height="81dp"
        android:layout_marginTop="208dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangleshape" />

    <ImageView
        android:id="@+id/passwordRegContainer"
        android:layout_width="295dp"
        android:layout_height="83dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EmailContainer"
        app:layout_constraintVertical_bias="0.022"
        app:srcCompat="@drawable/rectangleshape" />

    <ImageView
        android:id="@+id/confirmPasswordContainer"
        android:layout_width="295dp"
        android:layout_height="83dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordRegContainer"
        app:layout_constraintVertical_bias="0.038"
        app:srcCompat="@drawable/rectangleshape" />

    <EditText
        android:id="@+id/EmailReg"
        android:layout_width="268dp"
        android:layout_height="48dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/segoe_ui"
        android:inputType="textEmailAddress"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/EmailContainer"
        app:layout_constraintEnd_toEndOf="@+id/EmailContainer"
        app:layout_constraintStart_toStartOf="@+id/EmailContainer"
        app:layout_constraintTop_toTopOf="@+id/EmailContainer"
        app:layout_constraintVertical_bias="0.484" />

    <EditText
        android:id="@+id/usernameReg"
        android:layout_width="268dp"
        android:layout_height="48dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="17dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/segoe_ui"
        android:inputType="textEmailAddress"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/usernameContainer"
        app:layout_constraintEnd_toEndOf="@+id/usernameContainer"
        app:layout_constraintStart_toStartOf="@+id/usernameContainer"
        app:layout_constraintTop_toTopOf="@+id/usernameContainer" />

    <EditText
        android:id="@+id/PasswordReg"
        android:layout_width="269dp"
        android:layout_height="55dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/segoe_ui"
        android:inputType="textPassword"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/passwordRegContainer"
        app:layout_constraintEnd_toEndOf="@+id/passwordRegContainer"
        app:layout_constraintStart_toStartOf="@+id/passwordRegContainer"
        app:layout_constraintTop_toTopOf="@+id/passwordRegContainer" />

    <EditText
        android:id="@+id/passwordConfirm"
        android:layout_width="269dp"
        android:layout_height="55dp"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="14dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/segoe_ui"
        android:inputType="textPassword"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/confirmPasswordContainer"
        app:layout_constraintEnd_toEndOf="@+id/confirmPasswordContainer"
        app:layout_constraintStart_toStartOf="@+id/confirmPasswordContainer"
        app:layout_constraintTop_toTopOf="@+id/confirmPasswordContainer"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/EmailTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/segoe_bold"
        android:text="E-mail adress"
        android:textColor="#828282"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/EmailReg"
        app:layout_constraintEnd_toEndOf="@+id/EmailReg"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/EmailReg"
        app:layout_constraintTop_toTopOf="@+id/EmailReg"

        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/usernameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="159dp"
        android:layout_marginBottom="14dp"

        android:fontFamily="@font/segoe_bold"
        android:text="User Name"
        android:textColor="#828282"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/usernameReg"
        app:layout_constraintEnd_toEndOf="@+id/usernameReg"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/usernameReg"
        app:layout_constraintTop_toTopOf="@+id/usernameReg"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/passwordTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/segoe_bold"
        android:text="Password"
        android:textColor="#828282"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/PasswordReg"
        app:layout_constraintEnd_toEndOf="@+id/passwordRegContainer"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="@+id/passwordRegContainer"
        app:layout_constraintTop_toTopOf="@+id/PasswordReg"
        app:layout_constraintVertical_bias="0.513" />

    <TextView
        android:id="@+id/passwordConfirmTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/segoe_bold"
        android:text="Confirm Password"
        android:textColor="#828282"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/passwordConfirm"
        app:layout_constraintEnd_toEndOf="@+id/passwordConfirm"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/passwordConfirm"
        app:layout_constraintTop_toTopOf="@+id/passwordConfirm"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/forgotPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/segoe_bold"
        android:text="Do you agree with our terms and conditions?"
        android:textColor="#91CDDA"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordRegContainer"
        app:layout_constraintVertical_bias="0.515" />

    <TextView
        android:id="@+id/loginTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="39dp"
        android:background="@drawable/roundcorner"
        android:backgroundTint="#325961"
        android:fontFamily="@font/segoe_bold"
        android:paddingLeft="40dp"
        android:paddingTop="3dp"
        android:paddingRight="40dp"
        android:paddingBottom="3dp"
        android:text="Register"
        android:textColor="#91CDDA"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/forgotPassword"
        app:layout_constraintVertical_bias="0.0" />

    <CheckBox
        android:id="@+id/agreeCheckBox"
        android:layout_width="30dp"
        android:layout_height="33dp"
        android:buttonTint="#91CDDA"
        android:checked="false"
        app:layout_constraintBottom_toBottomOf="@+id/forgotPassword"
        app:layout_constraintEnd_toStartOf="@+id/forgotPassword"
        app:layout_constraintTop_toTopOf="@+id/forgotPassword" />

</androidx.constraintlayout.widget.ConstraintLayout>